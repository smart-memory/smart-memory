  "ontology": {
    "enabled": false,
    "inference": {
      "engine": "basic",
      "ontogpt": {
        "enabled": false,
        "provider": "openai",
        "model": "gpt-5-mini",
        "api_base": "",
        "api_version": "",
        "template": "",
        "template_dir": "",
        "confidence_threshold": 0.6
      }
    }
  },
  "extractor": {
    "default": "llm",
    "gliner2": {
      "model_name": "fastino/gliner2-base-v1",
