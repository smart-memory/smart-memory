    "persist": true,
    "vectorize": false,
    "retention_days": 14,
    "bypass_pipeline": true
  },
  "vector": {
    "default": "falkordb",
    "type": "falkordb",
    "backend": "falkordb",
    "dimension": 1536,
    "metric": "cosine",
    "hnsw_m": 16,
    "hnsw_ef_construction": 200,
    "hnsw_ef_runtime": 64,
    "host": "localhost",
    "port": 9010,
