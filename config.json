    },
    "gpt4o_triple": {
      "model_name": "gpt-5-mini",
      "openai_api_key": ""
    }
  },
  "enricher": {
    "default": "default",
    "temporal": {
      "model_name": "gpt-5-mini",
      "openai_api_key": ""
    }
  },
  "cache": {
    "host": "localhost",
    "redis": {
      "host": "localhost",
      "port": 9012,
      "db": 0,
      "password": "",
      "socket_timeout": 5,
      "socket_connect_timeout": 5,
      "retry_on_timeout": true,
      "health_check_interval": 30
    },
    "ttl": {
      "embedding_ttl": 86400,
      "search_ttl": 900,
      "entity_extraction_ttl": 3600,
      "similarity_ttl": 1800
    }
  },
  "mongodb": {
    "host": "localhost",
    "port": 9013,
    "database": "smartmemory"
  },
  "background": {
    "mode": "redis"
  },
  "adapter": {
    "default": "default"
  },
  "converter": {
    "default": "default"
  },
  "observability": {
    "enabled": true,
    "unified_keys": true
  },
  "auth": {
    "provider": "jwt",
    "jwt": {
      "secret_key": "",
      "algorithm": "HS256",
      "access_token_expire_minutes": 1440,
      "issuer": "smart-memory"
    },
    "oauth": {
      "google": {
        "client_id": "",
        "client_secret": ""
      }
    },
    "database": {
      "user_table": "users",
      "connection_string": ""
    }
  },
  "analytics": {
    "sentiment": {
      "enabled": true,
      "backend": "vader"
    },
    "topic": {
      "enabled": true,
      "model": "lda",
      "n_topics": 15,
      "min_df": 2,
      "max_features": 20000,
      "min_docs": 10
    },
    "bias": {
      "js_alert_threshold": 0.3,
      "protected_attributes_default": "gender,race,age_group,nationality,religion"
    }
  },
  "ssg": {
    "enabled": true,
    "default_algorithm": "query_traversal",
    "max_results": 15,
    "min_results_before_early_stop": 8,
    "neighbor_candidates": {
      "graph_neighbors": 10,
      "vector_neighbors": 10,
      "max_total": 20
    },
    "caching": {
      "similarity_cache_ttl": 1800,
      "traversal_path_cache_ttl": 900
    },
    "algorithms": {
      "query_traversal": {
        "enabled": true
      },
      "triangulation_fulldim": {
        "enabled": true
      }
    }
  }
